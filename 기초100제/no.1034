#include <stdio.h>

/* 8진 정수 입력받아 10진수로 출력하기 */

int main() {
    // %o (8진수 형태) : 한 자리에 8개의 문자 사용
    // %d (10진수 형태) 출력 : 한 자리에 10개의 문자 사용
    
    int num;
    scanf("%o",&num); // 8진수 형태로 입력
    printf("%d",num); // 10진수 형태로 출력
    
    // (C언어에서 소스 코드 작성 시 0으로 시작하는 수는 8진수로 인식된다. int a = 013;
    // 10진수 11과 같은 값)
}
