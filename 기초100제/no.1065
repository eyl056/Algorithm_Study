#include <stdio.h>

/* [기초-조건/선택실행구조] 정수 3개 입력받아 짝수만 출력하기 */
/*
 (a>b ? a:b)>c ? (a>b ? a:b):c 의 계산식은
 a, b, c 의 값 중 가장 큰 값으로 계산된다.
 */


int main() {
    int a,b,c;
    scanf("%d %d %d",&a,&b,&c); // 세 수 입력받기
    
    // 짝수인 경우 출력하기
    if(a%2==0) {
        printf("%d\n",a);
    }
    if(b%2==0) {
        printf("%d\n",b);
    }
    if(c%2==0) {
        printf("%d\n",c);
    }
}
