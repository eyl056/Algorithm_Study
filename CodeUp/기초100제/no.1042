#include <stdio.h>

/* 문자 1개 입력받아 다음 문자 출력하기 */

int main() {
    // -2147483648 <= a <= b <= +2147483647, b는 0이 아니다.
    int a, b; // 정수 2개
    scanf("%d %d",&a,&b); // 정수 2개 입력 받기
    if(b==0) {
        printf("다시 입력해주세요\n");
        return 0;
    }
    
    printf("%d",a/b); // a를 b로 나눈 몫 출력
    
    // (float)a -> 명시적 데이터 형변환
}
