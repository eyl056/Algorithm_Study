#include <stdio.h>

/* 정수 2개 입력받아 나눈 나머지 출력하기 */

int main() {
    // 0 <= a, b <= +2147483647, b는 0이 아니다.
    int a, b; // 정수 2개
    scanf("%d %d",&a,&b); // 정수 2개 입력 받기
    if( a<0 || b<0 || b==0) {
        printf("다시 입력해주세요\n");
        return 0;
    }
    
    printf("%d",a%b); // 정수%정수 연산의 결과는 나눈 나머지
}
