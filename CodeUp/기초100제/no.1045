#include <stdio.h>

/* 정수 2개 입력받아 자동 계산하기 */

int main() {
    // 0 <= a, b <= 2147483647, b는 0이 아니다.
    // a, b => int 형
    long long int a, b; // 정수 2개
    scanf("%lld %lld",&a,&b); // 입력 받기 // 더해서 int형 범위가 넘어가기 떄문에 long long int형
    
    printf("%lld\n",a+b); // 합
    if(a>=b) { // 차
        printf("%lld\n",a-b);
    }
    else {
        printf("%lld\n",a-b);
    }
    printf("%lld\n",a*b); // 곱
    printf("%lld\n",a/b); // 몫
    printf("%lld\n",a%b); // 나머지
    printf("%.2f\n",(double)a/(double)b); // 나눈 값
}
