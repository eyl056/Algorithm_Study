#include <stdio.h>

/* [기초-조건/선택실행구조] 정수 1개 입력받아 평가 출력하기 */
/*

 */


int main() {
    int a;
    scanf("%d",&a); // 점수 입력받기
    
    if(a>=90) {
        printf("A");
    }
    else if (a>=70){
        printf("B");
    }
    else if (a>=40) {
        printf("C");
    }
    else {
        printf("D");
    }
}
